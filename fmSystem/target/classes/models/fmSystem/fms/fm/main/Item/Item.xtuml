	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	0,
	'

//select any chapter from instances of Chapter where selected.Year == param.Year; 
select any item from instances of Item where selected.ItemID==param.ItemID; 

if ( empty item ) and (param.Action=="NEW")
	LOG::LogInfo(message:"Attempting to add a new item to employee." );

	//create object instance item of budget;
	create object instance i of Item;
	i.ItemID = param.ItemID;
	i.Fund = param.Fund;
	i.Status = param.Status;
	i.Type = param.Type;
	i.Category = param.Category;
	
	select any section from instances of Section where selected.SecID == param.SecID;
	relate i to section across R3;  


	//SQL::serialize();
	Fin::Reply( msg:"budget created successfully.", state:True);

elif ( not empty item ) and (param.Action=="NEW")
	LOG::LogInfo(message:"Attempting to add a new item." );

	LOG::LogInfo(message:"Item already exists." );	
	Fin::Reply( msg:"Item already exists", state:False);

elif ( not empty item ) and (param.Action=="UPDATE")
	LOG::LogInfo(message:"Attempting to update item." );

	item.Status = param.Status;
	item.Fund = param.Fund;
	item.Type = param.Type;
	item.Category = param.Category;
	
	LOG::LogInfo(message:"Item updated successfully." );	
	Fin::Reply( msg:"Item updated successfully", state:True);

elif ( not empty item ) and (param.Action=="DELETE")
	LOG::LogInfo(message:"Attempting to delete a item instance." );
	LOG::LogInfo(message:"Item delete in not implemented yet." );	
	Fin::Reply( msg:"Item delete in not implemented yet", state:False);

 elif (empty item )
	LOG::LogInfo(message:"Item does not exist." );	
	Fin::Reply( msg:"Item does not exist.", state:False);



end if;


