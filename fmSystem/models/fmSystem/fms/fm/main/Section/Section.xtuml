	"ba5eda7a-def5-0000-0000-000000000000",
	0,
	'

//select any chapter from instances of Chapter where selected.Year == param.Year; 
select any section from instances of Section where selected.SecID==param.SecID; 

if ( empty section ) and (param.Action=="NEW")
	LOG::LogInfo(message:"Attempting to add a new section to employee." );

	//create object instance section of budget;
	create object instance s of Section;
	s.Name = param.Name;
	s.Fund = param.Fund;
	s.Code = param.Code;
	s.Economic_Category = param.Economic_Category;
	s.Comments = param.Comments;
	
	select any chapter from instances of Chapter where selected.Year == param.Year;
	relate s to chapter across R2;  


	//SQL::serialize();
	Fin::Reply( msg:"budget created successfully.", state:True);

elif ( not empty section ) and (param.Action=="NEW")
	LOG::LogInfo(message:"Attempting to add a new section." );

	LOG::LogInfo(message:"Section already exists." );	
	Fin::Reply( msg:"Section already exists", state:False);

elif ( not empty section ) and (param.Action=="UPDATE")
	LOG::LogInfo(message:"Attempting to update section." );

	section.Name = param.Name;
	section.Fund = param.Fund;
	section.Code = param.Code;
	section.Economic_Category = param.Economic_Category;
	section.Comments = param.Comments;
	
	LOG::LogInfo(message:"Section updated successfully." );	
	Fin::Reply( msg:"Section updated successfully", state:True);

elif ( not empty section ) and (param.Action=="DELETE")
	LOG::LogInfo(message:"Attempting to delete a section instance." );
	LOG::LogInfo(message:"Section delete in not implemented yet." );	
	Fin::Reply( msg:"Section delete in not implemented yet", state:False);

 elif (empty section )
	LOG::LogInfo(message:"Section does not exist." );	
	Fin::Reply( msg:"Section does not exist.", state:False);



end if;


