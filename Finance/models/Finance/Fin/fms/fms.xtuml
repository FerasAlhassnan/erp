	'ReadChapters',
	'',
	'select any budget from instances of Budget where selected.Year == param.BudgetYear; 
select many chapters related by budget->Chapter[R100.''divided''];

if (not empty chapters) 
	LOG::LogInfo(message:"Sending chapter list .." );	
	
	for each chapter in chapters
			UI::SendChapters( Code:chapter.Code, Name:chapter.Name, Ceiling:chapter.Ceiling, Requested:chapter.Requested);		
			LOG::LogInfo(message:"Sent: Chapter: "+ chapter.Code+ ", "+chapter.Name);
			LOG::LogInfo(message:", Cieling: "); LOG::LogInteger( message:chapter.Ceiling );
			LOG::LogInfo(message:", Requested: "); LOG::LogInteger( message:chapter.Requested );	
	end for;

  LOG::LogInfo(message:"Sending chapter list is complete" );	
  
  else 
  	LOG::LogInfo(message:"There are no chapters registered in the system" );	

  	UI::Reply( msg:"There are no chapters registered in the system", state:False );  
  end if; ',
	1,
	0);
