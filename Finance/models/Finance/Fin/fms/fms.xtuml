INSERT INTO SPR_PS
	VALUES ("fa6c80d5-002a-4a73-8291-6ea0b8458836",
	'OperationalizeBudget',
	'',
	'select any budgetD from instances of BudgetD where selected.Year == param.Year;
create object instance journald of JournalD;
relate budgetD to journald across R11;



select any budget from instances of Budget where selected.Year == param.Year;

select many expenses related by budget->Chapter[R100.''divided'']->Expenses[R111.''funds''];

select any journal from instances of Journal where selected.Name == param.Journal;

//subledger;
select any apLedger from instances of Ledger where selected.Name == param.APLedger;
select any cashLedger from instances of Ledger where selected.Name == param.CashLedger;

//select many ledgers related by journal->DoubleEntryColumn[R131.''has'']->SubLedger[R129.''debits''];//->Chapter[R100.''divided'']->Expenses[R111.''funds''];
 
if (not empty expenses)
	for each expense in expenses
		create object instance account of Account;
		relate expense to account across R117.''provides funds to'';
		create object instance ap of SubLedger;
		create object instance cash of SubLedger;
		relate ap to apLedger across R134.''belongs to'';
		relate cash to cashLedger across R134.''belongs to'';
		select any doubleEntry  related by journal->DoubleEntryColumn[R131.''has'']; 
		relate ap to doubleEntry across R130.''credited by'';
		relate cash to doubleEntry across R129.''debited by'';
	end for;

end if;',
	1,
	0);
