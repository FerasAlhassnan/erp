INSERT INTO SPR_PS
	VALUES ("2fb2b152-5a14-4b7b-950b-d414de821b21",
	'ReadSectionsRequested',
	'',
	'select any budget from instances of Budget where selected.Year == param.BudgetYear; 
select any chapter related by budget->Chapter[R100.''divided''] where selected.Code==param.ChapterCode;

if (not empty chapter) 
	LOG::LogInfo(message:"Sending section requested list .." );	
	select many sectionsRequests related by chapter->Expenses[R115.''requests''];
	//select any section related by sectionsRequests->Section[R105.''specified by''];
if ( not empty sectionsRequests) //and (not empty section)
	for each sectionR  in sectionsRequests
			select one section related by sectionR->Section[R105.''specified by''];
			UI::SendSectionsRequested( Code:section.Code, Name:section.Name, RequestedFund:sectionR.Fund, RequestedRevenue:sectionR.Revenue );
			LOG::LogInfo(message:"Sent: Section: "+ section.Code+ ", "+section.Name);
			LOG::LogInfo(message:", Requested Fund: "); LOG::LogInteger( message:sectionR.Fund );
			LOG::LogInfo(message:", Requested Revenue: "); LOG::LogInteger( message:sectionR.Revenue );	
	
	end for;
  LOG::LogInfo(message:"Sending section requested complete" );	
  else 
  	
  	LOG::LogInfo(message:"There are no sections registered in the system" );	
  	UI::Reply( msg:"There are no sections registered in the system", state:False );  

  	
  end if;

  else 
  	LOG::LogInfo(message:"There are no sections registered in the system" );	
  	UI::Reply( msg:"There are no sections registered in the system", state:False );  
 end if;',
	1,
	0);
