INSERT INTO SPR_PS
	VALUES ("4d4a7767-746c-40a2-be26-f334e0c6728c",
	'ReviewSection',
	'',
	'
select any section from instances of Section where selected.Code == param.Code; 

if (not empty section)
	select many odlExpense related by section->Expenses[R105.''specifies''];
	select many newExpense related by section->Expenses[R106.''sepecifies new''];

	if ( not empty odlExpense)
		for each request in odlExpense
			generate Expenses2:Review to request;
		end for;
 
	elif (not empty newExpense)
		for each request in newExpense
			generate Expenses2:Review to request;
		end for;
	else
		LOG::LogInfo(message:"No expense instance found for section: "+ section.Name);

	end if;

else
		LOG::LogInfo(message:"No section found ");

end if;
',
	1,
	0);
