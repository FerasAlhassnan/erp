INSERT INTO SPR_PS
	VALUES ("68ec02e2-2e5f-46c0-98b2-f3f945c8e339",
	'ReadBudgets',
	'',
	'select many budgets from instances of Budget; 

if (not empty budgets) 
	LOG::LogInfo(message:"Sending budget list .." );	
	
	for each budget in budgets
			UI::SendBudgets( Year:budget.Year, Ceiling:budget.Ceiling, Requested:budget.Requested);		
			LOG::LogInfo(message:"Sent: Budget "+ budget.Year);
			LOG::LogInfo(message:", Cieling: "); LOG::LogInteger( message:budget.Ceiling );
			LOG::LogInfo(message:", Requested: "); LOG::LogInteger( message:budget.Requested );	
	end for;

  LOG::LogInfo(message:"Sending budget list is complete" );	
  
  else 
  	LOG::LogInfo(message:"There are no budgets registered in the system" );	

  	UI::Reply( msg:"There are no budgets registered in the system", state:False );  
  end if; ',
	1,
	0);
