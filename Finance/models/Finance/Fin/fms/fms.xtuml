INSERT INTO SPR_PS
	VALUES ("0cfd372b-9920-434a-a0a5-5eab902b7658",
	'ReadExpenditureAccounts',
	'',
	'select any budget from instances of Budget where selected.Year == param.BudgetYear; 
select many accounts related by budget->Account[R1.''assigned''];

if (not empty accounts) 
	LOG::LogInfo(message:"Sending account list .." );	
	
	for each account in accounts
		select one specaccount related by account->AccountSpecification[R20.''budgetized by'']->AccountSpecification[R8.''part''];
		if (empty specaccount)
			select one expenditure related by account->AccountSpecification[R20.''budgetized by'']->Expenditure[R18];
			UI::SendChapters( Code:specaccount.Code, Name:specaccount.Name, Ceiling:expenditure.CeilingFund, Requested:expenditure.RequestedFund);		
			//LOG::LogInfo(message:"Sent: Chapter: "+ chapter.Code+ ", "+chapter.Name);
			//LOG::LogInfo(message:", Cieling: "); LOG::LogInteger( message:chapter.Ceiling );
			//LOG::LogInfo(message:", Requested: "); LOG::LogInteger( message:chapter.Requested );	
		end if;
	end for;

  LOG::LogInfo(message:"Sending chapter list is complete" );	
  
  else 
  	LOG::LogInfo(message:"There are no chapters registered in the system" );	

  	UI::Reply( msg:"There are no chapters registered in the system", state:False );  
  end if; ',
	1,
	0);
