//create object instance amount of Amount;
//amount.Value = param.Amount;

//relate amount to entry across R124.''registered by'';
//relate amount to doubleEntry across R132.''is recorded by'';
create object instance account of AccountSpecification;
create object instance exp of Expenditure;
create object instance prog of Program;

relate account to exp across R18;
//relate account to prog across R18;

select any journal from instances of JournalD where selected.Name == "Salary";
create object instance entry of EntryD;
entry.Date = param.Date;
//entry.Desecription = param.Description;
create object instance amount of Amount;
amount.Amount = param.Amount;
relate entry to amount across R4;

select one column related by journal->Column[R16.''has single''];
relate amount to column across R13;
select one debit related by column->AccountSpecification[R14.''debits'']->AccountD[R17.''operates''];
select one credit related by column->AccountSpecification[R15.''credits'']->AccountD[R17.''operates''];

relate amount to debit across R3.''debits'';
relate amount to credit across R5.''credits'';
',
